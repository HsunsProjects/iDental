<Window x:Class="iDental.Views.ComboPics.ComboPic4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:iDental.Views.ComboPics"
        mc:Ignorable="d"
        Title="組圖(四張)" WindowState="Maximized">
    <Grid ShowGridLines="True">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Image Grid.Column="0" Grid.Row="0" x:Name="ComboPicImage1"/>
        <Image Grid.Column="1" Grid.Row="0" x:Name="ComboPicImage2"/>
        <Image Grid.Column="0" Grid.Row="1" x:Name="ComboPicImage3"/>
        <Image Grid.Column="1" Grid.Row="1" x:Name="ComboPicImage4"/>
    </Grid>
</Window>
